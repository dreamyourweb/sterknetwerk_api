			<div class="kracht"><h4>Uw kracht achter de schermen</h4></div>
		</div>

		<div class="page">
			<h2>Contact</h2>
			<div class="contact-container">
				<h3>Sterknetwerk B.V.</h3>
				<dl>
					<dt>Post Adres:</dt>
						<dd>Postbus 334 </dd>
						<dd>5280AH Boxtel </dd>
					<dt>Bezoek Adres:</dt>
						<dd>St. Lambertusweg 52 </dd>
						<dd>5291AC Gemonde </dd>
					<dt>Telefoon:</dt>
						<dd>073-5510565</dd>
					<dt>Fax:</dt>
						<dd>073-5510564</dd>
					<dt>Mail:</dt>
						<dd>info@sterknetwerk.nl </dd>
					<dt>KvK:</dt>
						<dd>17148758 's-Hertogenbosch</dd>
				</dl>
				<div class="spacer"></div>
				<h3 id="company-name"><%= @result.company_name %></h3>
				<dl>
					<dt>Contact persoon:</dt>
						<dd id="client-name"><%= @result.contact_name %></dd>
					<dt>Telefoon:</dt>
						<dd id="client-tel"><%= @result.tel %></dd>
					<dt>Mail:</dt>
						<dd id="client-email"><%= @result.email %></dd>
				</dl>
			</div>
			<div style="clear:both"></div>
			<div class="footer"><div class="footer-bar"></div></div>
		</div>

		<% Aspect.all.each_with_index do |aspect,aspect_i| %>
			<%# if aspect.aspects.include? aspect_i %>
				<div class="page">
					<h2><%= aspect.title %></h2>
					<% aspect.indicators.each do |indicator| %>
						<div style="clear:both"></div>
						<div class="aspect-image" style="<%= image_name_to_css(indicator.image_name) %>;"><div class="passfail <%= indicator.pass ? "pass" : "fail" %>"></div></div>
						<div class="aspect-text">
							<h3><%= indicator.title %></h3>
							<p><%= indicator.description %></p>
						</div>
					<% end %>
					<div style="clear:both"></div>
					<div class="footer"><div class="footer-bar"></div></div>
				</div>
			<%# end %>
		<% end %>


		<!--<page_cost>-->			
		<div class="page">
			<h2>Kosten</h2>
			<div class="cost-container">
